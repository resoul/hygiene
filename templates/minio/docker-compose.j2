  minio:
    image: minio/minio:latest
    command: server /data --console-address ":9001"
    ports:
      - "9000:9000"
      - "9001:9001"
    environment:
      MINIO_ROOT_USER: hygiene_user
      MINIO_ROOT_PASSWORD: hygiene_password
      DOMAINS: minio.hygiene.localhost
    volumes:
      - volume_storage:/data